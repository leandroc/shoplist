  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.25",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "classnames": "^2.3.1",
    "firebase": "^9.6.7",
    "history": "^5.3.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^5.0.2",
