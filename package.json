    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:emulator": "firebase emulators:exec 'npm run start' --only=auth,firestore,storage --import=./scripts/firebase/emulators/data --ui",
    "build": "react-scripts build",
    "test": "react-scripts test --env=./jest.override.js --detectOpenHandles",
    "test:emulator": "firebase emulators:exec 'npm run test -- --coverage' --only=auth,firestore,storage --import=./scripts/firebase/emulators/data --ui",
    "eject": "react-scripts eject",
    "emulators": "firebase emulators:start --import=./scripts/firebase/emulators/data --export-on-exit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "jest-environment-jsdom": "^27.5.1"
  }
